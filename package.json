{
  "name": "todos",
  "version": "1.0.0",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc",
    "test": "jest",
    "e2e": "start-server-and-test start 3000 e2e:run",
    "e2e:run": "yarn cypress run",
    "e2e:open": "yarn cypress open",
    "migrate:make": "yarn migrate --compiler=\"ts:./migrate-compiler.js\" --store=\"./migrate-store.ts\" create",
    "migrate:up": "yarn migrate --compiler=\"ts:./migrate-compiler.js\" --store=\"./migrate-store.ts\" up",
    "migrate:down": "yarn migrate --compiler=\"ts:./migrate-compiler.js\" --store=\"./migrate-store.ts\" down"
  },
  "dependencies": {
    "@emotion/core": "^10.0.28",
    "@google-cloud/firestore": "^3.7.5",
    "@reach/auto-id": "^0.10.1",
    "dotenv": "^8.2.0",
    "emotion": "^10.0.27",
    "emotion-server": "^10.0.27",
    "faunadb": "^2.14.0",
    "firebase": "^7.14.3",
    "firebase-admin": "^8.12.1",
    "fs": "^0.0.1-security",
    "isomorphic-unfetch": "3.0.0",
    "knex": "^0.21.1",
    "lodash": "^4.17.15",
    "next": "latest",
    "pg": "^8.0.3",
    "pg-connection-string": "^2.2.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "rhdf": "^0.2.9",
    "uuidv4": "^6.0.8"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.9.0",
    "@testing-library/cypress": "^6.0.0",
    "@testing-library/jest-dom": "^5.5.0",
    "@testing-library/react": "^10.0.2",
    "@types/jest": "^25.2.1",
    "@types/lodash": "^4.14.150",
    "@types/node": "^12.12.21",
    "@types/react": "^16.9.16",
    "@types/react-dom": "^16.9.4",
    "@types/testing-library__cypress": "^5.0.5",
    "cypress": "^4.5.0",
    "cypress-dotenv": "^1.2.0",
    "esm": "^3.2.25",
    "jest": "^25.3.0",
    "migrate": "^1.6.2",
    "prettier": "^2.0.4",
    "start-server-and-test": "^1.11.0",
    "ts-node": "^8.10.1",
    "typescript": "3.7.3"
  },
  "prettier": {
    "singleQuote": true,
    "semi": false,
    "trailingComma": "es5"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "./test-utils/setupTests.ts"
    ],
    "modulePathIgnorePatterns": [
      "cypress"
    ],
    "collectCoverageFrom": [
      "<rootDir>/src/**"
    ]
  },
  "license": "ISC"
}
